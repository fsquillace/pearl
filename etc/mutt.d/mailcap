application/pdf ; /usr/bin/zathura %s ; copiousoutput
image/* ; /usr/bin/mirage %s ; copiousoutput
audio/* ; /usr/bin/mplayer %s ; copiousoutput
video/* ; /usr/bin/mplayer %s ; copiousoutput

